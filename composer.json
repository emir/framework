{
    "name": "linna/framework",
    "type": "library",
    "description": "Linna Framework",
    "keywords": ["best-pratices", "framework", "learning", "mvc-framework", "php"],
    "homepage": "https://github.com/linna/framework",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian Rapetti",
            "email": "sebastian.rapetti@alice.it"
        }
    ],
    "support": {
        "issues": "https://github.com/linna/framework/issues"
    },
    "require": {
        "php": "^7.0.0",
        "linna/typed-array" :  "^1.0",
        "psr/container": "^1.0",
        "s3b4stian/simple-cache": "^1.0"
    },
    "require-dev": {
        "mongodb/mongodb": "^1.1.0",
        "phpmetrics/phpmetrics": "^2.3",
        "phpstan/phpstan": "^0.8.5"
    },
    "autoload": {
        "files": [
            "src/Linna/Support/Helpers.php"
        ],
        "psr-4": {
            "Linna\\": "src/Linna/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Linna\\Foo\\": "tests/FooClass/" 
        }
    }
}
